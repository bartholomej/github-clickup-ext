{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "short_name": "github-cu",
  "author": "bartholomej",
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
